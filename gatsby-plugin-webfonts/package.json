{
  "name": "gatsby-plugin-webfonts",
  "version": "2.0.0",
  "description": "A Gatsby plugin to handle cdn, base64 and self hosted webfonts",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/hupe1980/gatsby-plugin-webfonts"
  },
  "keywords": [
    "gatsby-plugin",
    "gatsby",
    "plugin",
    "web",
    "fonts",
    "loader",
    "google",
    "webfonts",
    "webfontloader"
  ],
  "main": "index.js",
  "scripts": {
    "build": "babel src --out-dir . --ignore **/__tests__",
    "build: watch": "babel -w src --out-dir . --ignore **/__tests__",
    "prepare": "cross-env NODE_ENV=production npm run build",
    "jest": "jest",
    "jest:watch": "jest --watch"
  },
  "peerDependencies": {
    "gatsby": "^3.0.0"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "cssnano": "^4.1.10",
    "fs-extra": "^9.0.1",
    "lodash.isempty": "^4.4.0",
    "postcss": "^8.2.2",
    "postcss-js": "^3.0.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "babel-jest": "^26.6.3",
    "babel-preset-gatsby-package": "^1.0.0",
    "cross-env": "^7.0.3",
    "jest": "^26.6.3"
  }
}
